RewriteEngine on
RewriteBase /
RewriteCond %{REQUEST_FILENAME} !-f
#RewriteCond %{REQUEST_FILENAME}  -f [OR]
#RewriteCond %{REQUEST_FILENAME} !-f
#RewriteCond %{REQUEST_FILENAME}  -d [OR]
#RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule !\.(js|gif|jpg|jpeg|bmp|png|css|txt|swf|eot|svg|ttf|woff|woff2|pdf|map|doc|docx|rtf|ico)$ index.php [QSA,L]



